{
  "name": "kitchen-timer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "style-loader": "^3.2.1",
    "styled-components": "^5.3.0",
    "typescript": "^4.4.3"
  },
  "homepage": "./",
  "scripts": {
    "template": "node scripts/build.template.js",
    "start": "yarn reset; yarn template; webpack-dev-server --open",
    "build": "yarn reset; yarn template; webpack --progress --color --config webpack.prd.config.js",
    "analyze": "yarn reset; yarn template; webpack --config webpack.prd-analyze.config.js",
    "reset": "rm -rf dist; mkdir dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.1.7",
    "compression-webpack-plugin": "^9.0.0",
    "mustache": "^4.2.0",
    "ts-loader": "^9.2.5",
    "css-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "webpack": "^5.53.0",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^4.2.1"
  }
}
